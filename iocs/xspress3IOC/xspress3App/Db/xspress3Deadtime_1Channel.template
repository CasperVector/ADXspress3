# max and mean deadtimes for a 1 Channel detector

record(calcout, "$(P)MeanDeadTime_RBV") {
    field(INPA, "$(P)C1:DeadTime_RBV NPP NMS")
    field(CALC, "1.0*(A)")
    field(PREC, "5")
}

record(calcout, "$(P)MaxDeadTime_RBV") {
    field(INPA, "$(P)C1:DeadTime_RBV NPP NMS")
    field(CALC, "1.0*(A)")
    field(PREC, "5")
}

